{
  "manifest_version": 3,
  "name": "Chromium JSON Editor",
  "description": "JSON Editor Extension for Chromium Browsers.",
  "version": "1.1.0",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/img/code-json-16.png",
      "32": "/img/code-json-32.png",
      "48": "/img/code-json-48.png",
      "128": "/img/code-json-128.png"
    }
  }
}
